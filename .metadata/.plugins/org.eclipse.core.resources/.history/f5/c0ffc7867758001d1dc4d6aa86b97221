				{0,0,7,0,4,0,2,0,3}
				
				
		};
		System.out.println(Arrays.deepToString(board));
		for(int i=0;i<9;i++) {
			
			for(int j=0;j<9;j++) {
				System.out.print(board[i][j]);
			}
			System.out.println();
		}
		
		
	}
